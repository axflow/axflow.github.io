---
interface Props {
  size?: string;
  className?: string;
}

const { className, size } = Astro.props;

const finalSize = size || 24;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width=`${finalSize}`
  height=`${finalSize}`
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}><path d="m9 18 6-6-6-6"></path></svg
>
